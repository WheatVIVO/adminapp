<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

    <profiles>
        <profile>
            <id>defaults</id>
            <properties>
                <app-name>wheatvivo-adminapp</app-name>

                <vivo-dir>/opt/portailble/vivo-home/wheatvivo-adminapp</vivo-dir>
                <tomcat-dir>/opt/portailble/tomcat.portailble</tomcat-dir>

                <default-theme>wheat</default-theme>
            </properties>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>defaults</activeProfile>
    </activeProfiles>
</settings>
